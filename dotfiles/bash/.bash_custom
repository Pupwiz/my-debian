# TNL: custom .profile, explicitly invoked from .bash_profile and .bashrc
# So, I put here all my customizations like PATH, etc.

export GPG_TTY=$(tty)
eval "$(lesspipe)"

#export QT_STYLE_OVERRIDE=Adwaita-Dark
export QT_STYLE_OVERRIDE=Adwaita

export GOPATH="$HOME/go"
export GEM_HOME=~/.ruby

export ANDROID_SDK_ROOT=/home/nick/Android/SDK
export CHROME_EXECUTABLE=/usr/local/bin/chromium

export PATH="$GOPATH/bin:$ANDROID_SDK_ROOT/cmdline-tools/latest/bin:$ANDROID_SDK_ROOT/platform-tools:$PATH:$GEM_HOME/bin"

source <(gopass completion bash)
